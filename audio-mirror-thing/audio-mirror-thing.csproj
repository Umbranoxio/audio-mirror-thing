<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-windows7.0</TargetFramework>
	<LangVersion>latest</LangVersion>
    <RootNamespace>AudioMirrorThing</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <StartupObject>AudioMirrorThing.Program</StartupObject>
    <PackageProjectUrl>https://github.com/umbranoxio/audio-mirror-thing</PackageProjectUrl>
    <RepositoryUrl>https://github.com/umbranoxio/audio-mirror-thing</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <FileVersion>1.0.0.0</FileVersion>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <Version>1.0.0</Version>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <Authors>Umbranox</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NAudio" Version="2.2.1" />
  </ItemGroup>

</Project>
